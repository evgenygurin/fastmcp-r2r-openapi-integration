# R2R API Configuration
R2R_BASE_URL=http://localhost:7272
R2R_API_KEY=your_api_key_here

# Optional: Custom OpenAPI spec URL (if not set, uses R2R_BASE_URL/openapi.json)
# Useful if you want to use a specific OpenAPI schema version or custom endpoint
# R2R_OPENAPI_URL=https://api.136-119-36-216.nip.io/openapi.json

# Optional: Request timeout in seconds (default: 30.0)
R2R_TIMEOUT=30.0

# Optional: Enable debug logging (default: false)
# Shows detailed OpenAPI parser and request building information
DEBUG_LOGGING=false

# Optional: Enable experimental FastMCP OpenAPI parser (recommended)
# The new parser has better performance, compatibility, and serverless support
# See: https://gofastmcp.com/integrations/openapi
FASTMCP_EXPERIMENTAL_ENABLE_NEW_OPENAPI_PARSER=true
